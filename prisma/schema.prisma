generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Work {
  id         Int      @id @default(autoincrement())
  name       String
  time_start String
  time_end   String
  status     String
  typeWork   String
  dates       String
  save       DateTime @default(now())
  updateAt   DateTime @updatedAt
}
